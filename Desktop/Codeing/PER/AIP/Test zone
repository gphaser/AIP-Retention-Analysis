# Sample DataFrame
data = {
    'Category': ['A', 'B', 'A', 'B', 'A', 'B'],
    'Value': [1, 2, 3, 4, 5, 6]
}
df = pd.DataFrame(data)

# Grouping by the 'Category' column
grouped = df.groupby('Category')

# Iterate over groups and print each group
for name, group in grouped:
    print(f"Group: {name}")
    print(group)
    print()


import matplotlib.pyplot as plt

# Sample data
categories = ['A', 'B', 'C', 'D', 'E']
values = [10, 20, 15, 25, 30]

# Plot bar graph
plt.bar(categories, values)

# Add data labels above each bar
for i, value in enumerate(values):
    plt.text(i, value + 1, str(value), ha='center', va='bottom')

# Add labels and title
plt.xlabel('Categories')
plt.ylabel('Values')
plt.title('Bar Graph with Data Labels')

# Show plot
plt.show()


import numpy as np
import matplotlib.pyplot as plt

# Sample data
categories = ['A', 'B', 'C', 'D', 'E']
values1 = [10, 20, 15, 25, 30]
values2 = [15, 25, 20, 30, 35]

# Set the width of the bars
bar_width = 0.35

# Set the positions of the bars on the x-axis
x = np.arange(len(categories))

# Plot bars for values1
plt.bar(x - bar_width/2, values1, width=bar_width, label='Values 1')

# Plot bars for values2
plt.bar(x + bar_width/2, values2, width=bar_width, label='Values 2')

# Add labels and title
plt.xlabel('Categories')
plt.ylabel('Values')
plt.title('Grouped Bar Graph')
plt.xticks(x, categories)
plt.legend()

# Show plot
plt.show()
