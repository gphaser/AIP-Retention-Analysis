# Sample DataFrame
data = {
    'Category': ['A', 'B', 'A', 'B', 'A', 'B'],
    'Value': [1, 2, 3, 4, 5, 6]
}
df = pd.DataFrame(data)

# Grouping by the 'Category' column
grouped = df.groupby('Category')

# Iterate over groups and print each group
for name, group in grouped:
    print(f"Group: {name}")
    print(group)
    print()
